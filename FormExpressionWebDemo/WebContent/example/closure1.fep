function foo(x) {
  return (i)=>{
    var a = x+i;
    return (j) => {
      return a*j+x;
    };
  };
}
m1 = foo(0);  // x=0
n1 = m1(5);   // x=0,  i=5,  a=5
m2 = foo(11); // x=11
n3 = m2(15);  // x=11, i=15, a=26
n2 = m1(10);  // x=0,  i=10, a=10
n4 = m2(20);  // x=11, i=20, a=31
[n1(2),n2(2),n3(2),n4(2),  // [10,20,63,73
 n1(4),n2(4),n3(4),n4(4)]; //  20,40,115,135]