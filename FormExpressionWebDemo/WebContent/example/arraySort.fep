function arraySort(array) {
  len = array.length;
  for (i = 0; i < len; i+=1) {
    for (j=i+1; j<len; j+=1) {
      tmp1 = array[i];
      tmp2 = array[j];
      if (tmp1 > tmp2) {
        array[i] = tmp2;
        array[j] = tmp1;
      }
    }
  }
  array;
}

// Of course, in practice it would be easier to
// use in-build functions:
// ["Anton","Alfred","Andre","Abigel"].sort;
arraySort(["Anton","Alfred","Andre","Abigel"]);