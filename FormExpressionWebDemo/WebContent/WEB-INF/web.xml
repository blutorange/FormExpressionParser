<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>FormExpressionWebDemo</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>EvaluateServlet</servlet-name>
		<servlet-class>de.xima.fc.form.expression.webdemo.EvaluateServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet>
		<servlet-name>FormatServlet</servlet-name>
		<servlet-class>de.xima.fc.form.expression.webdemo.FormatServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

    <servlet>
        <servlet-name>UnformatServlet</servlet-name>
        <servlet-class>de.xima.fc.form.expression.webdemo.UnformatServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

</web-app>