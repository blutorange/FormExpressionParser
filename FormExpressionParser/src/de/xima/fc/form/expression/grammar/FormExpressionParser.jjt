    |
      (
        < AngleClose > PlusLikeExpression(EMethod.ANGLE_CLOSE)
      )
    |
      (
        < AngleOpenEqual > PlusLikeExpression(EMethod.ANGLE_OPEN_EQUAL)
      )
    |
