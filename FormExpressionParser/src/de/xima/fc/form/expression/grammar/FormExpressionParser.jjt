| 
  < Percent : "%" >
| 
  < LosBodyClose : "%]" > : LOS
| 
  < MultiCommentBegin : "/*" > : MULTI_LINE_COMMENT
| 
  < ScopeSeparator : "::" >
| 
  < LambdaArrow : "->" >
| 
  < DoublePlus : "++" >
