// Computes the first n prime numbers.
// Uses the ...'s sieve.
scope algo {
  function primeNumbers(max) {
    if (max < 2) return [];
    var m = max;
    var arr = [];
    for (n : m) arr.push(n);
    arr.push(m);
    for (var n = 2; n <= max; ++n) {
      for (var r = n+n; r <= m; r+=n) {
        arr[r] = -1;
      }
    }
    var primes = [];
    for (n : max-1) {
      if (arr[n+2]>=0) {
        primes.push(n+2);
      }
    }
    return primes;
  }
}